  "version": "0.0.1",
  "scripts": {
    "build": "yarn build:css && yarn build:remix && yarn build:rollup",
    "build:remix": "remix build",
    "build:rollup": "rollup -c",
    "build:css": "postcss ./styles/index.css --output app/styles/index.css --env production",
    "dev": "concurrently \"yarn dev:remix\" \"yarn dev:css\"",
    "dev:remix": "remix dev",
    "dev:css": "TAILWIND_MODE=watch postcss ./styles/index.css --output app/styles/index.css --watch",
    "postinstall": "remix setup node"
  },
  "dependencies": {
    "@headlessui/react": "^1.4.2",
    "@heroicons/react": "^1.0.5",
    "@remix-run/react": "^1.6.5",
    "@remix-run/serve": "^1.6.5",
    "@remix-run/vercel": "^1.6.5",
    "@remix-validated-form/with-zod": "*",
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-json": "^4.1.0",
    "classnames": "^2.3.1",
    "framer-motion": "^5.5.5",
    "highlight.js": "^11.3.1",
    "lodash": "^4.17.21",
    "nanoid": "^3.3.4",
    "outdent": "^0.8.0",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "rehype-highlight": "^5.0.1",
    "remix": "^1.2.1",
    "remix-validated-form": "*",
    "zod": "^3.11.6",
    "zod-form-data": "*"
  },
  "devDependencies": {
