    "autoprefixer": "^10.4.0",
    "classnames": "^2.3.1",
    "concurrently": "^6.5.1",
    "custom-remark-plugin": "*",
    "postcss": "^8.4.5",
    "postcss-cli": "^9.1.0",
    "postcss-import": "^14.0.2",
    "rollup": "^2.63.0",
    "tailwindcss": "3.0.7",
    "typescript": "^4.1.2"
  },
  "engines": {
    "node": ">=14"
  },
